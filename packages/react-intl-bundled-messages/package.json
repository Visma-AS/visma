{
  "name": "@visma/react-intl-bundled-messages",
  "version": "0.1.6",
  "description": "`IntlProvider` that lazy loads messages in current language in Webpack environment.",
  "homepage": "https://github.com/Visma-AS/visma/tree/main/packages/react-intl-bundled-messages",
  "repository": {
    "type": "git",
    "url": "https://github.com/Visma-AS/visma.git"
  },
  "license": "ISC",
  "author": {
    "name": "Arno Saine",
    "email": "arno.saine@visma.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.js",
      "default": "./lib/index.js"
    },
    "./lib": {
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.js",
      "default": "./lib/index.js"
    },
    "./lib/*": {
      "import": "./lib/*",
      "require": "./lib/cjs/*",
      "default": "./lib/*"
    },
    "./*": {
      "import": "./lib/*.js",
      "require": "./lib/cjs/*.js",
      "default": "./lib/*.js"
    }
  },
  "types": "./lib/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build": "tsc && tsc --module commonjs --outDir lib/cjs && echo \"{}\" > lib/cjs/package.json",
    "start": "tsc --watch"
  },
  "dependencies": {
    "@postinumero/use-async": "^0.3.2",
    "@visma/react-app-locale-utils": "^0.2.0",
    "react-intl": "^5.20.10"
  },
  "devDependencies": {
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
